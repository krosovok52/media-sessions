[book]
title = "Media Sessions Documentation (RU)"
authors = ["krosov_ok"]
language = "ru"
src = "ru"

[output.html]
git-repository-url = "https://github.com/krosovok52/media-sessions"
edit-url-template = "https://github.com/krosovok52/media-sessions/edit/main/docs/src/ru/{path}"
site-url = "/media-sessions/ru/"
